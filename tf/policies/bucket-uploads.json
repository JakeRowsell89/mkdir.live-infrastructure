{
    "Version":"2012-10-17",
    "Statement":[
      {
        "Sid":"UploadsBucketPolicyGetObject",
        "Effect":"Allow",
        "Principal": {
            "AWS": [
                "arn:aws:iam::236744700502:root"
            ]
        },
        "Action":[
            "s3:GetObject"
        ],
        "Resource": [
            "arn:aws:s3:::mkdir.live-uploads/*"
        ]
      }
    ]
  }